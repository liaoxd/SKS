<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="${relativePackage}.${activityClass}"
    android:background="@drawable/bg"
    >
    <TextView
        android:id="@+id/sksname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="30dp"
        android:text="OA"
        android:textColor="#FFF"
        android:textSize="24dp"
        android:textStyle="bold"
        />
    <LinearLayout
        android:id="@+id/enterll1"
        android:layout_width="match_parent"
        android:layout_height="81dp"
        android:layout_below="@id/sksname"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:orientation="horizontal"
        android:background="#00000000"
        >
        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="81dp"
            android:layout_weight="2"
            >
            <TextView
                android:id="@+id/enternews"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/news"
                />
            <TextView
                android:id="@+id/enternewsindicator"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true"
                android:textSize="10sp"
                android:textColor="#FFF"
                android:textStyle="bold"
                android:gravity="center"
                android:background="@drawable/newbg"
                />
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="81dp"
            android:layout_weight="1"
            >
            <TextView
                android:id="@+id/enterzone"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/zone"
                />
            <TextView
                android:id="@+id/enterzoneindicator"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true"
                android:textSize="10sp"
                android:textColor="#FFF"
                android:textStyle="bold"
                android:gravity="center"
                android:background="@drawable/newbg"
                />
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="81dp"
            android:layout_weight="1"
            >
            <TextView
                android:id="@+id/enterspill"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/spill"
                />
        </RelativeLayout>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/enterll2"
        android:layout_width="match_parent"
        android:layout_height="81dp"
        android:layout_below="@id/enterll1"
        android:layout_alignLeft="@id/enterll1"
        android:layout_alignRight="@id/enterll1"
        android:orientation="horizontal"
        android:background="#00000000"
        >
        <TextView
            android:id="@+id/entercontact"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/contact"
            />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/enterll3"
        android:layout_width="match_parent"
        android:layout_height="162dp"
        android:layout_below="@id/enterll2"
        android:layout_marginTop="15dp"
        android:layout_alignLeft="@id/enterll2"
        android:layout_alignRight="@id/enterll2"
        android:orientation="horizontal"
        android:background="#00000000"
        >
        <TextView
            android:id="@+id/entermeetingroom"
            android:layout_width="0dp"
            android:layout_height="162dp"
            android:background="@drawable/meetingroom"
            android:layout_weight="1"
            />
        <LinearLayout
            android:id="@+id/enterll3ll1"
            android:layout_width="0dp"
            android:layout_height="162dp"
            android:orientation="vertical"
            android:background="#00000000"
            android:layout_weight="1"
            >
            <LinearLayout
                android:id="@+id/enterll3ll2"
                android:layout_width="match_parent"
                android:layout_height="81dp"
                android:orientation="horizontal"
                >
                <TextView
                    android:id="@+id/enterattendance"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/attendance"
                    />
                <TextView
                    android:id="@+id/enterpayment"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/payment"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/enterll3ll3"
                android:layout_width="match_parent"
                android:layout_height="81dp"
                android:orientation="horizontal"
                >
                <TextView
                    android:id="@+id/enterhoilday"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/holiday"
                    />
                <TextView
                    android:id="@+id/enterhire"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/hire"
                    />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/enterll4"
        android:layout_width="match_parent"
        android:layout_height="81dp"
        android:layout_below="@id/enterll3"
        android:layout_alignLeft="@id/enterll3"
        android:layout_alignRight="@id/enterll3"
        android:orientation="horizontal"
        android:background="#00000000"
        >
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="81dp"
            >
            <TextView
                android:id="@+id/enterplan"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/plan"
                />
        </RelativeLayout>
    </LinearLayout>

    <ImageView
        android:id="@+id/entersetting"
        android:layout_width="26dp"
        android:layout_height="32dp"
        android:layout_alignParentRight="true"
        android:layout_marginRight="10dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="10dp"
        android:background="@drawable/setting"
        />"
</RelativeLayout>
